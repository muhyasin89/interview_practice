    "\n",
    "def makeArrayConsecutive2(statues):\n",
    "    rang = statues.sort()\n",
    "\n",
    "    some = 0\n",
    "\n",
    "    if len(rang)-1 == 0:\n",
    "        return 0\n",
    "\n",
    "    else:\n",
    "        i=0\n",
    "\n",
    "        for i in range(len(rang)-2):\n",
    "            some += rang[i+1] - rang[i] - 1\n",
    "\n",
    "        return some\n"
   ]
  }
